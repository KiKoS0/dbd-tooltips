{
  "name": "dbd-tooltips",
  "version": "1.0.1",
  "private": true,
  "description": "The official client for the Dead by Daylight Tooltips extention for Twitch.",
  "repository": "https://github.com/KiKoS0/dbd-tooltips",
  "type": "module",
  "files": [
    "dist/*",
    "dist/assets/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint -c .eslintrc.json './src/**/*.{ts,ts,svelte}'",
    "tc": "tsc --build",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@sentry/vite-plugin": "^2.20.1",
    "@sveltejs/vite-plugin-svelte": "^3.1.1",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^20.14.9",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "eslint": "^8.57.0",
    "eslint-plugin-svelte": "^2.41.0",
    "eslint-plugin-unused-imports": "^4.0.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "prettier-plugin-svelte": "^3.2.5",
    "rollup-plugin-webpack-stats": "^1.0.1",
    "svelte": "^5.0.0-next.166",
    "svelte-check": "^3.8.4",
    "tslib": "^2.6.3",
    "typescript": "^5.5.2",
    "vite": "^5.3.2"
  },
  "packageManager": "yarn@4.2.2",
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@sentry/svelte": "^8.13.0"
  }
