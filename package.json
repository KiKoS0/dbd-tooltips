{
  "name": "dbd-tooltips",
  "version": "1.0.0",
  "private": true,
  "description": "The official client for the Dead by Daylight Tooltips extention for Twitch.",
  "repository": "https://github.com/KiKoS0/dbd-tooltips",
  "type": "module",
  "files": [
    "dist/*",
    "dist/assets/*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint -c .eslintrc.json './src/**/*.{ts,ts,svelte}'",
    "tc": "tsc --build",
    "postinstall": "husky install"
  },
  "devDependencies": {
    "@sentry/vite-plugin": "^2.10.2",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tsconfig/svelte": "^5.0.0",
    "@types/node": "^20.3.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.42.0",
    "eslint-plugin-svelte": "^2.30.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "rollup-plugin-webpack-stats": "^0.2.2",
    "svelte": "^4.0.0",
    "svelte-check": "^3.3.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.2",
    "vite": "^5.0.0"
  },
  "packageManager": "yarn@4.0.2",
  "lint-staged": {
    "*": "prettier --write --ignore-unknown"
  },
  "dependencies": {
    "@sentry/svelte": "^7.86.0"
  }
